# FinSight Environment Variables
# Copy this file to .env and fill in your actual values

# Google Gemini API Key (required for LangExtract service)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Python environment settings
# PYTHONUNBUFFERED=1

# Optional: Backend service ports (defaults shown)
# DOCUMENT_CONVERTER_PORT=8000
# SENTIMENT_SERVICE_PORT=8001
# NER_SERVICE_PORT=8002
# LANGEXTRACT_SERVICE_PORT=8003

# Optional: Frontend configuration
# VITE_API_BASE_URL=http://localhost:8000
